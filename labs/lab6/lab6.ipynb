   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "def f(x, n):\n",
    "    return (x**2 + 5) % n\n",
    "n = 1359331\n",
    "a = 1; b = 1\n",
    "d = 1\n",
    "while d == 1:\n",
    "    a = f(a, n)\n",
    "    b = f(f(b, n), n)\n",
    "    d = np.gcd(a - b, n)\n",
    "if d == n:\n",
    "    print(\"Делитель не найден\")\n",
    "else: \n",
    "    print(\"Нетривиальный делитель числа:\", d)"
   ]
